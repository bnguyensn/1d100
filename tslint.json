{
  "extends": "tslint:recommended",
  "rules": {
    "curly": false,
    "max-line-length": [
      true,
      120
    ],
    "new-parens": true,
    "no-arg": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": false,
    "no-console": [
      true,
      "debug",
      "info",
      "time",
      "timeEnd",
      "trace"
    ],
    "no-trailing-whitespace": [
      false
    ],
    "object-literal-sort-keys": [
      true,
      "match-declaration-order"
    ],
    "quotemark": [
      true,
      "single",
      "jsx-double",
      "avoid-template",
      "avoid-escape"
    ],
    "semicolon": [
      false
    ]
  }
}